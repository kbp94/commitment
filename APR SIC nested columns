let
    Source = APR,
    Schema = Table.Schema(Source),
    ColTypes = Table.SelectColumns(Schema, {"Name","Kind"}),
    NestedCols = Table.SelectRows(ColTypes, each [Kind] = "Record" or [Kind] = "Table")
in
    NestedCols


let
    Source = SIC,
    Schema = Table.Schema(Source),
    ColTypes = Table.SelectColumns(Schema, {"Name","Kind"}),
    NestedCols = Table.SelectRows(ColTypes, each [Kind] = "Record" or [Kind] = "Table")
in
    NestedCols

