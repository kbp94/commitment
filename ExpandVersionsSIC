let
    Source = OData.Feed("https://your-site.sharepoint.com/_api/web/lists/getbytitle('SIC Call')/items?$expand=Versions", null, [Implementation="2.0"]),
    FilterValidRows = Table.SelectRows(Source, each [Versions] <> null and [Versions] <> ""),
    SetTableType = Table.TransformColumnTypes(FilterValidRows, {{"Versions", type table}}, "en-US"),
    ExpandedVersions = Table.ExpandTableColumn(
        SetTableType,
        "Versions",
        {"Modified", "AuthorId", "EditorId", "OData__UIVersionString"},
        {"Version.Modified", "Version.AuthorId", "Version.EditorId", "Version.UIVersionString"}
    ),
    HandleErrors = Table.ReplaceErrorValues(ExpandedVersions, {
        {"Version.Modified", null},
        {"Version.AuthorId", null},
        {"Version.EditorId", null},
        {"Version.UIVersionString", null}
    })
in
    HandleErrors
