let
    Source = YourSICCallTable, // Replace with SIC Call table
    SetTableType = Table.TransformColumnTypes(Source, {{"Versions", type table}}, "en-US"),
    FilterValidVersions = Table.SelectRows(SetTableType, each [Versions] <> null and [Versions] <> ""),
    ExpandedVersions = Table.ExpandTableColumn(
        FilterValidVersions,
        "Versions",
        {"Modified", "AuthorId", "EditorId", "OData__UIVersionString"},
        {"Version.Modified", "Version.AuthorId", "Version.EditorId", "Version.UIVersionString"}
    ),
    HandleErrors = Table.ReplaceErrorValues(ExpandedVersions, {
        {"Version.Modified", null},
        {"Version.AuthorId", null},
        {"Version.EditorId", null},
        {"Version.UIVersionString", null}
    })
in
    HandleErrors
